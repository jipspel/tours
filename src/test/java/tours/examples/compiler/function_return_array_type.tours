func boolean[] makeArrayBoolean(boolean i) {
    boolean[] j = {i, i};
    return j;
}

func character[] makeArrayCharacter(character i) {
    character[] j = {i, i};
    return j;
}

func integer[] makeArrayInteger(integer i) {
    integer[] j = {i, i};
    return j;
}

func string[] makeArrayString(string i) {
    string[] j = {i, i};
    return j;
}

func main() {
    boolean[] b1 = makeArrayBoolean(false);
    print(b1[1]);
    boolean[] b2 = new boolean[5];
    b2 = makeArrayBoolean(true);
    print(b2[1]);

    character[] c1 = makeArrayCharacter('a');
    print(c1[0]);
    character[] c2 = new character[5];
    c2 = makeArrayCharacter('b');
    print(c2[1]);

    integer[] i1 = makeArrayInteger(35);
    print(i1[0]);
    integer[] i2 = new integer[5];
    i2 = makeArrayInteger(36);
    print(i2[1]);

    string[] s1 = makeArrayString("Hello");
    print(s1[0]);
    string[] s2 = new string[5];
    s2 = makeArrayString("World!");
    print(s2[1]);
}
